{
  "name": "blog",
  "description": "Blog by Robert Ying",
  "scripts": {
    "develop": "yarn clean && gatsby develop",
    "build": "yarn clean && gatsby build",
    "clean": "rimraf .cache public",
    "lint": "concurrently \"yarn lint:js\" \"yarn lint:scss\"",
    "lint:js": "eslint --cache --ext .js,.jsx --ignore-pattern public .",
